{
  "hosting": {
    "public": "dist",
    "ignore": ["firebase.json", "**/.*", "**/node_modules/**"],
    "rewrites": [
      {
        "source": "**",
        "destination": "/index.html"
      }
    ]
  },
  "redirects": [
    {
      "source": "/home",
      "destination": "/protected/home",
      "type": 301
    }
  ]
}
